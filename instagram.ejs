<%- include("includes/head.ejs") %>    <!--include is a sub templates -->
  <body>
    <!-- THis is used for the  generating the instagram page. -->

    <h2>This is belong to <%= data.name%></h2>
    <button>Follow</button>
    <button>Message</button>

    <p>
      Followers : <%= data.followers %> &nbsp &nbsp; &nbsp; &nbsp; Following :
      <%= data.following %>
    </p>
    <hr />

    <% for(let post of data.posts) { %>
    <img src="<%= post.image  %>" alt="some img" />
    <p>
      Likes : <%= post.likes %> &nbsp &nbsp; &nbsp; &nbsp; Following : <%=
      post.comments %>
    </p>
    <% } %>

    <%- include("includes/footer.ejs")  %>
  </body>
  <script src="app.js"></script>
</html>
